{
  "name": "vusec",
  "version": "0.1.1",
  "description": "Wrap vue component to composable function.",
  "scripts": {
    "build": "pnpm run test && unbuild",
    "play": "pnpm run -C playground dev",
    "test": "vitest --run",
    "test:watch": "vitest -w",
    "release": "bumpp --no-push"
  },
  "keywords": [
    "vue3",
    "vue",
    "component"
  ],
  "author": {
    "name": "alwynou",
    "email": "alwynou@gmail.com"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "peerDependencies": {
    "vue": "^3.2.47"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@alwynou/eslint-config": "^0.27.2",
    "@vitejs/plugin-vue": "^4.2.1",
    "@vue/test-utils": "^2.3.2",
    "bumpp": "^9.1.0",
    "eslint": "^8.39.0",
    "happy-dom": "^9.10.9",
    "typescript": "^5.0.4",
    "unbuild": "^1.2.1",
    "vite": "^4.3.2",
    "vitest": "^0.31.0",
    "vue": "^3.2.47"
  }
}
